{
    "id":  "f0626509-7ca8-46db-95bf-853512c1e752",
    "name":  "ALOpsDockerRemove",
    "friendlyName":  "ALOps Docker Remove",
    "description":  "Remove a Business Central container",
    "author":  "Hodor",
    "helpMarkDown":  "Remove Business Central docker container.",
    "category":  "Utility",
    "visibility":  [
                       "Build",
                       "Release"
                   ],
    "demands":  [

                ],
    "version":  {
                    "Major":  1,
                    "Minor":  439,
                    "Patch":  2105
                },
    "minimumAgentVersion":  "1.95.0",
    "instanceNameFormat":  "ALOps Docker Remove",
    "inputs":  [
                   {
                       "name":  "usedocker",
                       "type":  "boolean",
                       "label":  "In Docker",
                       "defaultValue":  true,
                       "required":  false,
                       "helpMarkDown":  "Run task in Docker container. $(usedocker)"
                   },
                   {
                       "name":  "fixed_tag",
                       "type":  "string",
                       "label":  "Fixed Container Tag",
                       "defaultValue":  "",
                       "required":  false,
                       "helpMarkDown":  "Allows recycling of docker containers. $(fixed_tag)"
                   },
                   {
                       "name":  "print_logs",
                       "type":  "boolean",
                       "label":  "Print Logs",
                       "defaultValue":  true,
                       "required":  false,
                       "helpMarkDown":  "Print all container logs before remove. $(print_logs)"
                   },
                   {
                       "name":  "createeventlogbackup",
                       "type":  "pickList",
                       "label":  "Create EventLog Backup",
                       "defaultValue":  "disabled",
                       "options":  {
                                       "disabled":  "Disabled",
                                       "onfailure":  "On Failure",
                                       "always":  "Always"
                                   },
                       "required":  false,
                       "helpMarkDown":  "Create EventLog Backup from Container. $(createeventlogbackup)"
                   },
                   {
                       "name":  "createsqlbackup",
                       "type":  "pickList",
                       "label":  "Create SQL Backup",
                       "defaultValue":  "disabled",
                       "options":  {
                                       "disabled":  "Disabled",
                                       "onfailure":  "On Failure",
                                       "always":  "Always"
                                   },
                       "required":  false,
                       "helpMarkDown":  "Create SQL Backup from Container Database. $(createsqlbackup)"
                   },
                   {
                       "name":  "publishsqlbackup",
                       "type":  "boolean",
                       "label":  "Publish SQL Backup",
                       "defaultValue":  true,
                       "required":  false,
                       "helpMarkDown":  "Published the SQL Backup as Build-Artifact. $(publishsqlbackup)",
                       "visibleRule":  "createsqlbackup = true"
                   },
                   {
                       "name":  "sqlcompression",
                       "type":  "boolean",
                       "label":  "Use SQL Compression",
                       "defaultValue":  false,
                       "required":  false,
                       "helpMarkDown":  "Create Compressed SQL Backup (does not work on SQL-Express). $(sqlcompression)",
                       "visibleRule":  "createsqlbackup = true"
                   },
                   {
                       "name":  "zipsqlbackup",
                       "type":  "boolean",
                       "label":  "ZIP SQL Backup",
                       "defaultValue":  true,
                       "required":  false,
                       "helpMarkDown":  "ZIP the SQL Backup. $(zipsqlbackup)",
                       "visibleRule":  "createsqlbackup = true"
                   }
               ],
    "execution":  {
                      "PowerShell3":  {
                                          "target":  "79122b97-40ce-4aa9-b5f2-92e0a51c9f11.ps1"
                                      }
                  }
}
