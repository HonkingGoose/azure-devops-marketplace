{
    "id":  "6ccb97e4-cd3b-4f5c-82d0-3b733949d45d",
    "name":  "ALOpsAppCompiler",
    "friendlyName":  "ALOps App Compiler",
    "description":  "Compile AL Extension(s) for Business Central",
    "author":  "Hodor",
    "helpMarkDown":  "Compile Business Central extension(s) from AL code.",
    "category":  "Utility",
    "visibility":  [
                       "Build",
                       "Release"
                   ],
    "demands":  [

                ],
    "version":  {
                    "Major":  2,
                    "Minor":  0,
                    "Patch":  1950
                },
    "minimumAgentVersion":  "1.95.0",
    "instanceNameFormat":  "ALOps App Compiler",
    "inputs":  [
                   {
                       "name":  "artifactversion",
                       "type":  "string",
                       "label":  "Artifact Version",
                       "defaultValue":  "",
                       "required":  false,
                       "helpMarkDown":  "BC/NAV Version, eg: 9, 10.4, NAV2016, 16.4.24524. $(artifactversion)"
                   },
                   {
                       "name":  "artifacttype",
                       "type":  "pickList",
                       "label":  "Artifact Type",
                       "defaultValue":  "OnPrem",
                       "options":  {
                                       "OnPrem":  "OnPrem",
                                       "Sandbox":  "Sandbox"
                                   },
                       "required":  false,
                       "helpMarkDown":  "Set Artifact Type. $(artifacttype)"
                   },
                   {
                       "name":  "artifactcountry",
                       "type":  "string",
                       "label":  "Artifact Country",
                       "defaultValue":  "",
                       "required":  false,
                       "helpMarkDown":  "The Country for the Artifact. $(artifactcountry)"
                   },
                   {
                       "name":  "versionselect",
                       "type":  "string",
                       "label":  "Select Version",
                       "defaultValue":  "Latest",
                       "required":  false,
                       "helpMarkDown":  "The version to be selected from the Artifacts. $(versionselect)"
                   },
                   {
                       "name":  "alsourcepath",
                       "type":  "string",
                       "label":  "AL Source Path",
                       "defaultValue":  "$(System.DefaultWorkingDirectory)",
                       "required":  false,
                       "helpMarkDown":  " $(alsourcepath)"
                   },
                   {
                       "name":  "appversiontemplate",
                       "type":  "string",
                       "label":  "App Version",
                       "defaultValue":  "1.0.*.0",
                       "required":  true,
                       "helpMarkDown":  "Template for versioning Apps. \u0027*\u0027 is replaced by the current Build Number. $(appversiontemplate)"
                   },
                   {
                       "name":  "alcodeanalyzer",
                       "type":  "string",
                       "label":  "AL Code Analysis",
                       "defaultValue":  "",
                       "required":  false,
                       "helpMarkDown":  "AL Analyzer(s) used for compiling. (Example: CodeCop,UICop) $(alcodeanalyzer)"
                   },
                   {
                       "name":  "alcachepath",
                       "type":  "string",
                       "label":  "AL Cache Path",
                       "defaultValue":  "$(System.DefaultWorkingDirectory)",
                       "required":  false,
                       "helpMarkDown":  " $(alcachepath)"
                   },
                   {
                       "name":  "publishartifact",
                       "type":  "boolean",
                       "label":  "Publish Artifact",
                       "defaultValue":  true,
                       "required":  false,
                       "helpMarkDown":  "Publish generated App Artifact to DevOps. $(publishartifact)"
                   },
                   {
                       "name":  "failonwarnings",
                       "type":  "boolean",
                       "label":  "Fail on Warnings",
                       "defaultValue":  false,
                       "required":  false,
                       "helpMarkDown":  "Fail task when any warning occurs. $(failonwarnings)"
                   },
                   {
                       "name":  "storageaccount",
                       "type":  "string",
                       "label":  "Storage Account",
                       "defaultValue":  "",
                       "required":  false,
                       "helpMarkDown":  "Non Default Storage Account. $(storageaccount)"
                   },
                   {
                       "name":  "sastoken",
                       "type":  "string",
                       "label":  "SAS Token",
                       "defaultValue":  "",
                       "required":  false,
                       "helpMarkDown":  "SAS Token used to access Storage Account. $(sastoken)"
                   }
               ],
    "execution":  {
                      "PowerShell3":  {
                                          "target":  "f5074b02-87fb-4326-b78e-3a3fc0c9376e.ps1"
                                      }
                  }
}
