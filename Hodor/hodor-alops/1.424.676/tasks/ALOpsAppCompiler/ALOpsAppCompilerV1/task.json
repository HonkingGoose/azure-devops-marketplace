                   },
                   {
                       "name":  "vsix_download_path",
                       "type":  "string",
                       "label":  "VSIX Url",
                       "defaultValue":  "",
                       "required":  false,
                       "visibleRule":  "usedocker = false",
                       "helpMarkDown":  "Alternative VSIX download url. $(vsix_download_path)"
