    },
    "contributions": [
        {
            "id": "oc-cmd-task",
            "description": "Task to run user-defined oc command.",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/oc-cmd"
            }
        },
        {
            "id": "config-map-task",
