{
    "id": "1da752a0-03bf-429c-a239-b5f06d43d629",
    "name": "ALOpsLicenseImport",
    "friendlyName": "ALOps License Import",
    "description": "ALOps License Import",
    "author": "Hodor",
    "helpMarkDown": "This extension assists in automating DynamicsNAV App builds",
    "category": "Utility",
    "visibility": [
        "Build",
        "Release"
    ],
    "demands": [],
    "version": {
        "Major": "1",
        "Minor": "420",
        "Patch": "10"
    },
    "minimumAgentVersion": "1.95.0",
    "instanceNameFormat": "ALOps License Import",
    "inputs": [
        {
            "name": "usedocker",
            "type": "boolean",
            "label": "In Docker",
            "defaultValue": false,
            "required": false,
            "helpMarkDown": "Run command in Docker container."
        },
        {
            "name": "fixed_tag",
            "type": "string",
            "label": "Fixed Container Tag",
            "defaultValue": "",
            "required": false,
            "helpMarkDown": "Allows recycling of containers.",
            "visibleRule": "usedocker = true"
        },
        {
            "name": "nav_serverinstance",
            "type": "string",
            "label": "Server Instance",
            "defaultValue": "BC140",
            "required": true,
            "helpMarkDown": "NAV Server Instance used for symbols and compiling the app.",
            "visibleRule": "usedocker = false"
        },
        {
            "name": "license_path",
            "type": "filePath",
            "label": "FLF Path",
            "required": false,
            "defaultValue": "",
            "helpMarkDown": "Path of the script to execute. Must be a fully qualified path or relative to $(System.DefaultWorkingDirectory)."
        }
    ],
    "execution": {
        "PowerShell3": {
            "target": "Import-NAVMgtLicense.ps1"
        }
    }
}