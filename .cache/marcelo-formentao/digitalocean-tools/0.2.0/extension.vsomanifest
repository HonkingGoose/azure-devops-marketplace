{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/marceloavf/digitalocean-tools-vsts"
    },
    "scopes": [
        "vso.build_execute",
        "vso.serviceendpoint_manage"
    ],
    "contributions": [
        {
            "id": "digital-ocean-spaces-delete-task",
            "type": "ms.vss-distributed-task.task",
            "description": "Delete file or folder content of DigitalOcean Spaces Bucket",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/DigitalOceanSpacesDelete"
            }
        },
        {
            "id": "digital-ocean-spaces-upload-task",
            "type": "ms.vss-distributed-task.task",
            "description": "Upload file and folder content to DigitalOcean Spaces Bucket",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/DigitalOceanSpacesUpload"
            }
        }
    ],
    "contributionTypes": []
}