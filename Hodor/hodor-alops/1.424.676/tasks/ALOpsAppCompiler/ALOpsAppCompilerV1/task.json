                   {
                       "name":  "al_analyzer",
                       "type":  "string",
                       "label":  "AL Analyzer",
                       "defaultValue":  "",
                       "required":  false,
                       "helpMarkDown":  "AL Analyzer(s) used for compiling. (Example: CodeCop,UICop) $(al_analyzer)"
