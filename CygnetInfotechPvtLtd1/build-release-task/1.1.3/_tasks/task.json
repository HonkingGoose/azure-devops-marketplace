{
    "id": "12b85747-ff1c-62e7-9c4b-f49360eea7cb",
    "name": "TWProExtension",
    "friendlyName": "Testingwhiz Pro Plugin",
    "description": "build/release task to Run UI test with Testingwhiz",
    "helpMarkDown": "Version: 1.1.3 [More information] : (https://www.testing-whiz.com/integrations)",
    "category": "Utility",
    "author": "Cygnet Infotech",
    "version": {
        "Major": 1,
        "Minor": 1,
        "Patch": 3
    },
    "instanceNameFormat": "TestingWhiz Pro Plugin",
    "groups": [{
        "name": "rbtsettings",
        "displayName": "RBT Enabled Execution",
        "isExpanded": true
    }],
    "inputs": [{
            "name": "azkabanProjectId",
            "type": "number",
            "label": "Azkaban ProjectId",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "Project ID"
        },
        {
            "name": "serverURL",
            "type": "string",
            "label": "Server URL",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "Server URL"
        },
        {
            "name": "successEmails",
            "type": "string",
            "label": "Success Emails",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "Success Emails"
        },
        {
            "name": "failureEmails",
            "type": "string",
            "label": "Failure Emails",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "Failure Emails"
        },
        {
            "name": "successEmailsOverride",
            "type": "boolean",
            "label": "Success Emails Override",
            "required": false,
            "defaultValue": "false",
            "helpMarkDown": "Success Emails Override"
        },
        {
            "name": "failureEmailsOverride",
            "type": "boolean",
            "label": "Failure Emails Override",
            "required": false,
            "defaultValue": "false",
            "helpMarkDown": "Failure Emails Override"
        },
        {
            "name": "groupId",
            "type": "string",
            "label": "Group Id",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "Group Id"
        },
        {
            "name": "executors",
            "type": "number",
            "label": "Executors",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "Executors"
        },
        {
            "name": "executorCores",
            "type": "number",
            "label": "Executor Cores",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "executorCores"
        },
        {
            "name": "baseURL",
            "type": "string",
            "label": "Base URL",
            "defaultValue": "",
            "required": false,
            "helpMarkDown": "TestingWhiz BASEURL should starts with http://www... or https://www... ."
        },
        {
            "name": "targetType",
            "type": "pickList",
            "label": "Script Source",
            "defaultValue": "filePath",
            "options": {
                "filePath": "filePath",
                "folderPath": "folderPath"
            },
            "required": false,
            "helpMarkDown": "Target script source: File Path or Folder Path."
        },
        {
            "name": "filePath",
            "type": "filePath",
            "label": "Script Path",
            "visibleRule": "targetType = filePath",
            "required": true,
            "defaultValue": "",
            "helpMarkDown": "Path of the TestingWhiz automation script file."
        },
        {
            "name": "folderPath",
            "type": "filePath",
            "label": "Folder Path",
            "visibleRule": "targetType = folderPath",
            "required": true,
            "defaultValue": "",
            "helpMarkDown": "Path of the TestingWhiz automation script Folder."
        },
        {
            "name": "reportDploy",
            "type": "filePath",
            "label": "Report Deploy",
            "required": true,
            "defaultValue": "",
            "helpMarkDown": "Path for to save the TestingWhiz Report."
        },
        {
            "name": "browser",
            "type": "pickList",
            "label": "Browser",
            "defaultValue": "googleChrome",
            "options": {
                "googleChrome": "Google Chrome",
                "mozillaFirefox": "Mozilla Firefox",
                "internetExplorer": "Internet Explorer",
                "headlessExecution": "PhantomJS",
			     "edge": "Edge",
				"edgeChromimum":"Edge Chromium",
				"chromeHeadless":"Chrome (Headless)",
				"mozillaHeadless":"FireFox (Headless)"
            },
            "required": false,
            "helpMarkDown": "Select the browser you wish to run the script on."
        },
       {
            "name": "mailReport",
            "type": "boolean",
            "label": "Mail Report ?",
            "required": false,
            "defaultValue": "false",
            "helpMarkDown": "Send generated report to the mail. Note:- Please use output variable available in Output variable section in this plugin. If Output Variable section is not visible then use ${mailReportPath} to send report to mail."
        }
       
    ],
    "OutputVariables": [{
        "name": "reportDeployPath",
        "description": "storing generated report file path."
    }],
    "execution": {
        "Node": {
            "target": "index.js"
        }
    }
}