{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/sdavis3/CodeCoverageDashboardWidgets"
    },
    "contributions": [
        {
            "id": "MutationCoverageWidget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog",
                "testWidgetSampleOsb1.mutation-coverage-dashboard-widgets.MutationCoverageWidget.Configuration"
            ],
            "properties": {
                "name": "Mutation Coverage",
                "description": "Displays the percentage of mutation coverage for a build definition.",
                "catalogIconUrl": "img/CatalogIcon.png",
                "previewImageUrl": "img/preview1.png",
                "uri": "code-coverage1.html",
                "isNameConfigurable": true,
                "supportedSizes": [
                    {
                        "rowSpan": 1,
                        "columnSpan": 1
                    },
                    {
                        "rowSpan": 1,
                        "columnSpan": 2
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "MutationCoverageWidget.Configuration",
            "type": "ms.vss-dashboards-web.widget-configuration",
            "targets": [
                "ms.vss-dashboards-web.widget-configuration"
            ],
            "properties": {
                "name": "Mutation Coverage Configuration",
                "description": "Configures MutationCoverageWidget",
                "uri": "configuration.html"
            }
        }
    ],
    "scopes": [
        "vso.build",
        "vso.test"
    ],
    "contributionTypes": []
}