                   {
                       "name":  "failed_on_warnings",
                       "type":  "boolean",
                       "label":  "Fail on Warnings",
                       "defaultValue":  false,
                       "required":  false,
                       "helpMarkDown":  "Fail task when any warning occurs. $(failed_on_warnings)"
                   }
               ],
    "execution":  {
